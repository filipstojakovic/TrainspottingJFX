            prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="project.map.DialogController">
    <center>
        <TextArea fx:id="textArea" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER"/>
    </center>
    <bottom>
        <Button mnemonicParsing="false" onAction="#closeBtnClicked" text="Close" BorderPane.alignment="CENTER"/>
    </bottom>
</BorderPane>
